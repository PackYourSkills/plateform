<% case params['infos'] %>
  <% when 'photo' %>
    <%= render 'profile_picture_form' %>
  <% when 'quote' %>
    <%= render 'quote_form' %>
  <% when 'values' %>
    <%= render 'values_form' %>
  <% when 'website' %>
    <%= render 'website_form' %>
  <% when 'cv' %>
    <%= render 'cv_form' %>
  <% when 'youtube' %>
    <%= render 'youtube_form' %>
  <% else %>
    <%= render 'form' %>
<% end %>
