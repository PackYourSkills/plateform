<%= simple_form_for @user, url: infos_update_path do |f| %>
    <%= f.input :first_name %>
    <%= f.input :last_name %>

    <%= f.input :address %>
    <%= f.input :city %>
    <%= f.input :zip_code %>
    <%= f.input :country , as: :string %>

    <%= f.input :birthdate %>

    <%= f.button :submit %>
<% end %>
